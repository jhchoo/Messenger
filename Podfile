# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Messenger' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # ignore all warnings from all pods
  inhibit_all_warnings!
  
  # 구글 파이어베이스
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  
  
  # 페이스북 로그인
  pod 'FBSDKLoginKit'
  # 구글 로그인
  pod 'GoogleSignIn'
  
  # 프로젝트에 쉽게 채팅 추가하기 💬 https://github.com/MessageKit/MessageKit
  pod 'MessageKit'
  # 우아하고 간단한 진행 HUD https://github.com/JonasGessner/JGProgressHUD
  pod 'JGProgressHUD'
  # 모바일 데이터베이스 https://github.com/realm/realm-cocoa
  pod 'RealmSwift', :inhibit_warnings => true
  # 캐시를 지원하는 비동기 이미지 다운로더 https://github.com/SDWebImage/SDWebImage
  pod 'SDWebImage'
  
end
